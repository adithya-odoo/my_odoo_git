<?xml version="1.0" encoding="UTF-8" ?>
<templates id="template" xml:space="preserve">
    <t t-inherit="owl_workshop.input_box" t-inherit-mode="extension" owl="1">
        <xpath expr="//div[hasclass('InputDiv')]" position="inside">
            <button t-on-click="() => this.demofu(1)" id="plus">+</button>
            <t t-esc="state.value"/>
            <button t-on-click="() => this.demofu(-1)" id="minus">-</button>
            <button t-on-click="fetchsaleorder">Fetch Sale</button>
            <button t-on-click="opensaleorder">Open SaleOrder</button>
        </xpath>
    </t>
</templates>